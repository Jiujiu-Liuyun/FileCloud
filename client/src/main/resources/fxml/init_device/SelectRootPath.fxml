<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.zhangyun.filecloud.client.controller.registerdevice.SelectRootPathController">
   <children>
      <Label alignment="CENTER" prefHeight="50.0" prefWidth="168.0" text="请选择同步文件夹" />
      <Label fx:id="pathShowLabel" alignment="CENTER" layoutY="60.0" prefHeight="40.0" prefWidth="300.0" />
      <Button layoutX="202.0" layoutY="12.0" mnemonicParsing="false" onAction="#selectPath" text="Browse" />
      <Button layoutX="190.0" layoutY="120.0" mnemonicParsing="false" onAction="#next" prefHeight="25.0" prefWidth="60.0" text="下一步" />
      <Button layoutX="70.0" layoutY="120.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="60.0" text="取消" />
   </children>
</AnchorPane>
