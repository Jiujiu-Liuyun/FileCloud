<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.zhangyun.filecloud.client.controller.registerdevice.ConfirmController">
   <children>
      <Label alignment="CENTER" layoutY="50.0" prefHeight="30.0" prefWidth="80.0" text="同步文件夹" />
      <Label alignment="CENTER" layoutY="80.0" prefHeight="30.0" prefWidth="80.0" text="设备名称" />
      <Label fx:id="rootPathTextField" alignment="CENTER" layoutX="100.0" layoutY="50.0" prefHeight="30.0" prefWidth="200.0" />
      <Label fx:id="deviceNameTextField" alignment="CENTER" layoutX="100.0" layoutY="80.0" prefHeight="30.0" prefWidth="200.0" />
      <Label alignment="CENTER" layoutX="50.0" layoutY="10.0" prefHeight="30.0" prefWidth="200.0" text="请确认以下信息" />
      <Button layoutX="190.0" layoutY="120.0" mnemonicParsing="false" onAction="#ok" prefHeight="25.0" prefWidth="60.0" text="确认" />
      <Button layoutX="70.0" layoutY="120.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="60.0" text="上一步" />
   </children>
</AnchorPane>
